      "(^[bB]uild\\((.*)\\): ?)": "\\($2\\)",
      "(^[cC]hore: ?)": "",
      "(^[cC]hore\\((.*)\\): ?)": "\\($2\\)",
      "(^[cC][iI]: ?)": "",
      "(^[cC][iI]\\((.*)\\): ?)": "\\($2\\)",
      "(^[dD]ocs: ?)": "",
      "(^[dD]ocs\\((.*)\\): ?)": "\\($2\\)",
      "(^[fF]eat: ?)": "",
      "(^[fF]eat\\((.*)\\): ?)": "\\($2\\)",
      "(^[fF]ix: ?)": "",
      "(^[fF]ix\\((.*)\\): ?)": "\\($2\\)",
      "(^[pP]erf: ?)": "",
      "(^[pP]erf\\((.*)\\): ?)": "\\($2\\)",
      "(^[rR]efactor: ?)": "",
      "(^[rR]efactor\\((.*)\\): ?)": "\\($2\\)",
      "(^[rR]evert: ?)": "",
      "(^[rR]evert\\((.*)\\): ?)": "\\($2\\)",
      "(^[sS]tyle: ?)": "",
      "(^[sS]tyle\\((.*)\\): ?)": "\\($2\\)",
      "(^[tT]est: ?)": "",
      "(^[tT]est\\((.*)\\): ?)": "\\($2\\)"
    }
  }
}
